{
  "name": "@panarch/concerto",
  "version": "0.1.12",
  "scripts": {
    "start": "webpack-dev-server --colors --progress",
    "build": "webpack --entry ./src/index.js --output-filename ./build/concerto.js --output-library Concerto --output-library-target umd",
    "release": "cp ./build/*.js ./releases && cp ./build/*.js.map ./releases",
    "lint": "eslint src/ tests/",
    "generate:current": "slimerjs ./tools/generate_svg_images.js build ./build/images/current",
    "generate:blessed": "slimerjs ./tools/generate_svg_images.js releases ./build/images/blessed",
    "generate": "npm run generate:current && npm run generate:blessed",
    "diff": "./tools/visual_regression.sh",
    "test": "npm run lint && npm run build && npm run generate && npm run diff"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/panarch/concerto-private.git"
  },
  "author": "Taehoon Moon",
  "bugs": {
    "url": "https://github.com/panarch/concerto-private/issues"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^1.10.3",
    "slimerjs": "^0.906.2",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "peerDependencies": {
    "@panarch/allegretto": "^1.2.5-3.2"
  },
  "description": "A Javascript library for parsing and rendering MusicXML. Copyright (c) 2016 Taehoon Moon",
  "main": "releases/concerto.js",
  "dependencies": {},
  "keywords": [
    "musicxml",
    "vexflow",
    "music",
    "notation"
  ]
}
